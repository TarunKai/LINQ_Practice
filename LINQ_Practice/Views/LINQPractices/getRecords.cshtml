@using LINQ_Practice.Models;
@model IEnumerable<TblEmployee>


<!DOCTYPE html>
<html>
<head>
    <style>
        table {
            border-collapse: collapse;
        }

        th, td {
            border: 1px solid orange;
            padding: 10px;
            text-align: left;
        }
    </style>
    <meta name="viewport" content="width=device-width" />
    <title>Controller</title>
</head>
<body>
    <h4>Employee</h4>
    <hr />
    <table>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Email</th>
            <th>Age</th>
            <th>Salary</th>
            <th>Gender</th>
            <th>Department</th>
        </tr>
        @foreach (TblEmployee emp in Model)
        {
            <tr>
                <td>@emp.Id</td>
                <td>@emp.Name</td>
                <td>@emp.Email</td>
                <td>@emp.Age</td>
                <td>@emp.Salary</td>
                <td>@emp.Gender</td>
                <td>@emp.Department</td>
            </tr>
        }
    </table>
</body>
</html>